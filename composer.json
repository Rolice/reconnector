{
  "name": "rolice/reconnector",
  "description": "Connector replacement for Laravel 5.1+ that allows reconnection with a list of different MySQL hosts.",
  "type": "library",
  "version": "1.0",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Lyubomir Gardev",
      "email": "rolice@intellisys.org"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-pdo": "*",
    "illuminate/support": ">=5.1",
    "illuminate/database": ">=5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "Rolice\\Reconnector\\": "src",
      "Rolice\\Reconnector\\Connectors\\": "src/Connectors/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rolice\\Reconnector\\Test\\": "tests/"
    }
  }
}
